{
  "singleQuote": true,
  "semi": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "proseWrap": "always",
        "printWidth": 160
      }
    },
    {
      "files": "*.html",
      "options": {
        "proseWrap": "always",
        "printWidth": 160
      }
    }
  ]
}
